{
    "device_type": "qemu-aarch64",
    "health_check": false,
    "job_name": "qemu-aarch64-uefi-prebuilt-boot",
    "logging_level": "DEBUG",
    "timeout": 3600,
    "actions": [
        {
            "command": "deploy_linaro_image",
            "parameters": {
                "bootloadertype": "uefi",
                "image": "http://images.validation.linaro.org/functional-test-images/qemu-aarch64/vexpress64-openembedded_minimal-armv8-gcc-4.9_20140823-686.img.gz",
                "qemu_pflash": ["http://images.validation.linaro.org/functional-test-images/qemu-aarch64/flash0.img.gz",
                                "http://images.validation.linaro.org/functional-test-images/qemu-aarch64/flash1.img.gz"]
            }
        },
        {
            "command": "boot_linaro_image",
            "parameters": {
                "boot_cmds": [
                    "sendline 3",
                    "expect Choice:",
                    "sendline 1",
                    "expect Select the Boot Device:",
                    "sendline 1",
                    "expect File path of the EFI Application or the kernel:",
                    "sendline Image",
                    "expect Is your application an OS loader? [y/n]",
                    "sendline n",
                    "expect Arguments to pass to the EFI Application:",
                    "sendline debug uefi_debug ignore_loglevel rootwait root=/dev/vda2 rootfstype=ext4 rw  console=ttyAMA0 earlycon uefi_debug acpi=force",
                    "expect Description for this new Entry:",
                    "sendline LAVA",
                    "expect Choice:",
                    "sendline 7",
                    "expect Start:",
                    "sendline 2"
                ]
            }
        },
        {
            "command": "lava_command_run",
            "parameters": {
                "commands": [
                    "dmesg|grep 'rtc-efi rtc-efi: setting system clock to'"
                ],
                "timeout": 900
            }
        },
        {
            "command": "submit_results",
            "parameters": {
                "stream": "BUNDLE_STREAM",
                "server": "LAVA_SERVER"
            }
        }
    ]
}
